<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PreviousTransactions">

    <ImageView
        android:id="@+id/back2"
        android:layout_width="112dp"
        android:layout_height="67dp"
        app:srcCompat="@drawable/backbutton"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="446dp" />

    <Button
        android:id="@+id/placeholderTransaction"
        android:layout_width="286dp"
        android:layout_height="58dp"
        android:background="#cc0000"
        android:capitalize="none"
        android:text="€15.00   -    Lidl"
        android:textColor="@android:color/white"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="73dp" />

    <Button
        android:id="@+id/placeholderTransaction2"
        android:layout_width="286dp"
        android:layout_height="58dp"
        android:background="#cc0000"
        android:capitalize="none"
        android:text="€45.56   -    Tesco"
        android:textColor="@android:color/white"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="153dp" />

    <TextView
        android:id="@+id/previousTrText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Previous Trancasations"
        android:textColor="@android:color/black"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/lidllogo"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="66dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="70dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/tesco_logo"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="146dp" />
</androidx.constraintlayout.widget.ConstraintLayout>